    vivo:hasSubjectArea "EDUC"^^xsd:string ;
    vivo:shortDescription "Please see Undergraduate Calendar"^^xsd:string .

focudata:7680 vivo:Catalog 303 ;
    vivo:CourseCredits 3.0 ;
    vivo:Identification 7680 ;
    vivo:Title "Children, Families, and Social Policy"^^xsd:string ;
    vivo:hasSubjectArea "EDUC"^^xsd:string ;
    vivo:shortDescription """This course examines social policy as it relates to children and families in Canada. Policies may include such areas as children's rights, child care, education, health, and social welfare.





"""^^xsd:string .

focudata:7681 vivo:Catalog 305 ;
