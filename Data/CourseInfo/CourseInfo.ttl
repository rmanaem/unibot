    vivo:Identification 26663 ;
    vivo:Title "Existentialism"^^xsd:string ;
    vivo:hasSubjectArea "PHIL"^^xsd:string ;
    vivo:shortDescription """This course acquaints the student with the fundamentals of the existentialist movement as a philosophical perspective. Philosophers considered may include Kierkegaard, Nietzsche, Heidegger, Sartre, Merleau-Ponty, Jaspers, Marcel, and Berdyaev.





"""^^xsd:string .

focudata:26664 vivo:Catalog 389 ;
