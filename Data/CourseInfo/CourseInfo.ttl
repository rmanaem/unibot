    vivo:Title "NEW DIRECTIONS IN ANTH RES"^^xsd:string ;
    vivo:hasSubjectArea "ANTH"^^xsd:string ;
    vivo:shortDescription "Please see GRAD Calendar"^^xsd:string .

focudata:11384 vivo:Catalog 402 ;
