    vivo:Identification 25660 ;
    vivo:Title "Music from the Post-Romantic to the Present"^^xsd:string ;
    vivo:hasSubjectArea "MHIS"^^xsd:string ;
    vivo:shortDescription "Prerequisite: MHIS 203, 204. A study of music from the early-20th century to the present. The roots of current trends in music are followed through their growth into the widely diverse styles of today."^^xsd:string .

