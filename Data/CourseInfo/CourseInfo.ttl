focudata:49685 vivo:Catalog 311 ;
    vivo:CourseCredits 3.0 ;
    vivo:Identification 49685 ;
    vivo:Title "Metal Casting: Bronze and Aluminum I"^^xsd:string ;
    vivo:hasSubjectArea "SCUL"^^xsd:string ;
