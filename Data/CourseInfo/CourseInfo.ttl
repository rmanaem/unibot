    vivo:Identification 48323 ;
    vivo:Title "UNDER-CAMERA ANIMATION"^^xsd:string ;
    vivo:hasSubjectArea "FMAN"^^xsd:string ;
    vivo:shortDescription """An intensive, hands-on introduction to the art, practice, and multiple techniques of direct animation, creating, photographing and erasing
