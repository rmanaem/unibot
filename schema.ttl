
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dbr: <http://dbpedia.org/resource/>.
@prefix owl: <http://www.w3.org/2002/07/owl#>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix focu: <http://focu.io/schema#>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.

# Classes

focu:University
  a rdfs:Class;
  owl:sameAs dbr:university;
  rdfs:label "University"@en;
  rdfs:comment "An educational institution designed for instruction and examination of students in many branches of advanced learning and conferring degrees in various faculties."@en.

focu:Course
  a rdfs:Class;
  rdfs:label "Course"@en;
  rdfs:comment "A unit of teaching in a higher education institution."@en.

focu:Lecture
  a rdfs:Class;
  rdfs:label "Lecture"@en;
  rdfs:comment "An educational talk to students in a university."@en.

focu:Topic
  a rdfs:Class;
  rdfs:label "Topic"@en;
  rdfs:comment "A particular subject."@en.

focu:Student
  a rdfs:Class;
  rdfs:subClassOf foaf:Person;
  rdfs:label "Student"@en;
  rdfs:comment "A person who is studying at a university."@en.

# Properties

focu:name
  a rdfs:Property;
  owl:sameAs foaf:name;
  rdfs:label "Name"@en;
  rdfs:comment "A word or set of words by which a person or thing is referred to."@en.

focu:link
  a rdfs:Property;
  rdfs:label "Link"@en;
  rdfs:comment "Link to entry in DBpedia and/or Wikidata."@en.

focu:subject
  a rdfs:Property;
  rdfs:domain focu:Course;
  rdfs:range xsd:string;
  rdfs:label "Subject"@en;
  rdfs:comment "A word or set of words used to refer to course subject."@en.

focu:number
  a rdfs:Property;
  rdfs:label "Number"@en;
  rdfs:comment "An arithmetical value used to refer to course number or lecture number."@en.

focu:credits
  a rdfs:Property;
  rdfs:domain focu:Course;
  rdfs:label "Credits"@en;
  rdfs:comment "An arithmetical value used to refer to course credits."@en.

focu:description
  a rdfs:Property;
  rdfs:domain focu:Course;
  rdfs:range xsd:string;
  rdfs:label "Description"@en;
  rdfs:comment "A word or set of words used to describe a course."@en.

focu:outline
  a rdfs:Property;
  rdfs:domain focu:Course;
  rdfs:label "Outline"@en;
  rdfs:comment "A document used to communicate important information regarding a course."@en.




